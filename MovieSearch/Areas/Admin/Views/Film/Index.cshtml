@model IEnumerable<Film>

<div class="container">
    <div class="row pt-4 align-items-center pb-3 d-flex">
        <div class="col-6">
            <h2 class="text-primary">Films List</h2>
        </div>
    </div>
    <table class="table table-bordered table-striped table col-lg-100 text-center">
        <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name Category
            </th>
        </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model) 
        {
            <tr>
                <td>
                    @obj.Id
                </td>
                <td>
                    @obj.Title
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>